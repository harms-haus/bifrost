{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/assets/*": ["assets/*"],
      "@/types/*": ["types/*"],
      "@/types": ["types/index.ts"],
      "@/pages/*": ["pages/*"],
      "@/theme": ["theme/index.tsx"],
      "@/theme/*": ["theme/*"]
    },
    "strict": true,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client", "vike-react", "node"],
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "exclude": ["dist"]
}
